<% author = User.find(note.user_id) %>

<li>
  <label>Posted by <%= author.email %> <%= time_ago_in_words(note.created_at
  ) + " ago"%>
</br>
    <p>
      <pre>
        <%= note.body %>
      </pre>
      <%=
        button_to(
          "delete note", note_url(note), method: :delete
        ) if note.user_id == current_user.id %>
    </p>
  </label>
</li>
